{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.12.0/.schema/devbox.schema.json",
  "packages": [
    "nginx@latest",
    "git@latest",
    "dotnet-sdk_8@latest",
    "redis@latest",
    "nodejs@18",
    "nodePackages.pm2@latest",
    "vscode@latest",
    "azure-cli@latest"
